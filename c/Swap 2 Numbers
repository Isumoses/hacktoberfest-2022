#include<stdio.h>
int main() {
  double first, second, temp;
  printf("Enter 1st number: ");
  scanf("%lf", &first);
  printf("Enter 2nd number: ");
  scanf("%lf", &second);

  temp = first;

  first = second;
  
  second = temp;

  printf("\nAfter swapping, 1st number = %.2lf\n", first);
  printf("After swapping, 2nd number = %.2lf", second);
  return 0;
}
